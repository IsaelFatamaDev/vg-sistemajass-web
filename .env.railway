# Configuración para Railway (Development)
NODE_ENV=development
NG_CLI_ANALYTICS=false

# Puerto dinámico de Railway (se asigna automáticamente)
# PORT se asigna automáticamente por Railway

# Configuración de Angular CLI
NG_CLI_BROWSER=false
NG_CLI_BYPASS_ANALYTICS=true

# Configuración de la aplicación
APP_NAME=Sistema JASS
APP_VERSION=1.0.0-dev

# Configuración de hot reload
NG_LIVE_RELOAD=true
NG_WATCH=true

# URL base de la API (ajusta según tu backend)
# API_BASE_URL=https://tu-backend.railway.app/api

# Configuración de logging para desarrollo
LOG_LEVEL=debug
